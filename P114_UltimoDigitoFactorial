package Solved;
import java.util.Scanner;

public class P114_UltimoDigitoFactorial {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int count = sc.nextInt();
		
		for (int i = 0; i < count; i++) {

			int x = sc.nextInt();
			
			switch (x) {
			case 0:
			case 1:
				
				x = 1;
				break;
			case 2:
				
				x = 2;
				break;

			case 3:
				
				x = 6;
				break;

			case 4:
				
				x = 4;
				break;

			default:
				
				x = 0;
				break;
			}
			
			
			System.out.println( x );
		}

	}

}
